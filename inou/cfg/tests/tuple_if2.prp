x.__bits = 5
y.__bits = 3

x = 2
y = 7


A = (x, foo = y, bar = 4)   

if (A.foo == 7) {           
  A[1]  = 17                
  A.bar = 13                
  x = A.foo
}                            

%out1 = A[1] + A.bar // 17 + 13 = 30 = 0x1e

A[0] = 3
%out2 = A[0] + A.foo // 3 + 17 = 20 = 0x14

%out3 = x            // 17 = 0x11


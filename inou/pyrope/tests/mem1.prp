// code/mem1.prp
@a as __bits:3 __size:1024 __rdports:1
@b as @a __fwd:false  // without cycle fowarding
@cycle as __bits:8
I @a[0] == @cycle
prev_val = @cycle
@cycle += 1
@a[0] = @cycle
@b[0] = @cycle
I @a[0] == @cycle
I @a[0].__last == prev_val
I @b[0] == @b[0].__last == prev_val
%out = @a[0] + @b.0